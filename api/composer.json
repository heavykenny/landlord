{
  "name": "estate-elite/landlord",
  "description": "A simple API for a landlord to manage their properties and tenants",
  "type": "project",
  "require": {
    "php": "^8.0",
    "monolog/monolog": "^2.0",
    "guzzlehttp/guzzle": "^7.0",
    "ext-json": "*",
    "ext-pdo": "*",
    "firebase/php-jwt": "^6.9"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "platform": {
      "php": "7.4.3"
    }
  }
}
